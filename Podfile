    pod 'SAMKeychain'
    pod 'AppCenter'
  # Pods for SDCrew

  target 'SDCrewTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SDCrewUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
